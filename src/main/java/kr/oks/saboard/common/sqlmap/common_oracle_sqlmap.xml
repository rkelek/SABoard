<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap>
		<insert id="common.oracle.join" parameterClass="kr.oks.saboard.common.domain.MemberDomain">
		insert into sa_member (id,pw,name) values (#id#,#pw#,#name#);
	</insert>
	
	<select id="common.oracle.loginCheck" parameterClass="kr.oks.saboard.common.domain.MemberDomain" resultClass="kr.oks.saboard.common.domain.MemberDomain">
		select * from sa_member where id=#id# and pw=#pw#
	</select>
</sqlMap>